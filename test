PROGRAM factorial2
BEGIN
	STRING input := "Please enter an integer number: ";
	STRING eol := "\n";
	
	FUNCTION INT factorial(INT a, INT b, INT c, FLOAT d, FLOAT e)
	BEGIN
        INT ret;

        ret := (a * b) + c;


        WRITE(d);

        WRITE(e);

        RETURN ret;
	END

	FUNCTION INT main ()
	BEGIN
		INT c0, c1, c2;
        FLOAT f0, f1;
		INT result;
		WRITE(input);

		READ(c0);
		READ(c1);
		READ(c2);

		READ(f0);
		READ(f1);

        result := factorial(c0, c1, c2, f0, f1);

        WRITE(result);
		
		RETURN 0;
	END
END

